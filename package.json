    "query-string": "^8.1.0",
    "swiper": "^11.0.4",
    "vant": "^4.7.3",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.2",
    "@iconify-json/akar-icons": "^1.1.20",
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/ic": "^1.1.14",
    "@iconify/utils": "^2.1.11",
    "@types/node": "^20.9.0",
    "@unocss/eslint-config": "^0.57.4",
    "@unocss/eslint-plugin": "^0.57.4",
    "@unocss/reset": "^0.57.4",
    "@vant/auto-import-resolver": "^1.0.2",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue-macros/volar": "^0.17.2",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^8.53.0",
    "eslint-define-config": "^1.24.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "pnpm": "^8.10.5",
    "sass": "^1.69.5",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.12.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.4",
    "unocss-preset-useful": "^0.3.4",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.6.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
